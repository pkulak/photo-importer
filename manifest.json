{
  "name": "Photo Importer",
  "description": "Import photos from your camera to a local folder with ease.",
  "version": "1.0.4",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "permissions": [{
      "mediaGalleries": ["read", "allAutoDetected"] 
  }, "identity"],
  "oauth2": {
    "client_id": "416869675733-plof1094pd36u1sl308n15h045tvkv8b.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  },
  "icons": {"128": "icon.png"}
}